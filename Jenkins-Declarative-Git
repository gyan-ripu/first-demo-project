pipeline {
    agent any
    
    stages {
        stage('Git Checkout') {
            steps {
                git credentialsId: 'github', url: 'https://github.com/gyan-ripu/first-demo-project'
            }
        }
    stages {
        stage('Maven Build') {
            steps {
                sh "mvn clean package"
            }
        }
    }
}
